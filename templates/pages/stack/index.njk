{% include "../../partials/head/index.njk" %}

<div 
  id="app"
  class="p--2"
  data-o-save-deep="defaultSave"
  data-o-type="object"
>
  <div class="pack">
    <header class="navbar pack__header">
      <section class="navbar-section">
        <div 
          class="pack__name h2"

          data-i-editable="packName(text-single-line)"
          data-o-type="object"
          data-o-key-pack-name="{{ data.packName }}"
          data-w-key-pack-name="innerText"
        >{{ data.packName }}</div>
        <button 
          class="btn btn-primary"
          data-i-new="stack .stacks"
        >Add Stack</button>
      </section>
    </header>
    <div 
      class="stacks"
      data-o-key="stacks"
      data-o-type="list"
    >
      {% set stack = currentItem %}
      {% include "../../partials/stack/index.njk" %}
    </div>
  </div>
</div>

<script src="/dist/index.js"></script>

{% include "../../partials/footer/index.njk" %}



